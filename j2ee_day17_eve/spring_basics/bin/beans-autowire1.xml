<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- Definition for textEditor bean using autowire by type for single property -->
	<bean id="textEditor" class="com.app.simple1.TextEditor"
		autowire="byType" />


	<bean id="spellChecker123" class="com.app.simple1.PrimitiveSpellChecker" />
	<bean id="adv" class="com.app.simple1.AdvancedSpellChecker" />





</beans>
